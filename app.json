{
  "expo": {
    "name": "sports-score",
    "slug": "score",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leejaehyup.sport"
    },
    "android": {
      "package": "com.leejaehyup.sport"
    },
    "description": "sport score beta app",
    "githubUrl": "https://github.com/leejaehyup/sports-score"
  }
}
